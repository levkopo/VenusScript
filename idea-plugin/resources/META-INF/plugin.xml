<idea-plugin>
  <id>com.levkopo.vs.idea</id>
  <name>VenusScript</name>
  <version>1.0</version>
  <vendor email="levkopoag@gmail.com">levkopo</vendor>

  <description>VenusScript Language Plugin for IDEA IDE</description>

    <idea-version since-build="173.0"/>
  <depends>com.intellij.modules.platform</depends>

  <extensions defaultExtensionNs="com.intellij">
    <fileType name="VenusScript" implementationClass="org.intellij.sdk.language.VenusFileType"
              fieldName="INSTANCE" language="VenusScript" extensions="vs"/>
    <fileTypeFactory implementation="org.intellij.sdk.language.VenusFileTypeFactory"/>
  </extensions>

  <actions>
    <!-- Add your actions here -->
  </actions>

</idea-plugin>