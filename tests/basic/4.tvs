# VenusScript - Types example.
# This script is used for testing purposes.
# Basically, shows the value type of some variables,
# and asserts they are correct.

using std

a = "I <3 COMPUTER SCIENCE!"
b = 5.3547
c = -33
d = 'o'
e = false
f = true
g = *string
h = *any
i = null

println(*a)
println(*b)
println(*c)
println(*d)
println(*e)
println(*f)
println(*g)
println(*h)
println(*i)
println((*a) == (*string))
println((*b) == (*decimal))
println((*c) == (*int))
println((*d) == (*string))
println((*e) == (*bool))
println((*f) == (*bool))
println((*g) == (*type))
println((*h) == (*any))
println((*i) == (*any))
